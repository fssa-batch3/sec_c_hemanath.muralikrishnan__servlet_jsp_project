	<error-page>
		<error-code>400</error-code> <!-- Bad Request -->
		<location>/pages/error/error_page.jsp?error=400&amp;msg=Bad+Request.+Please+check+your+request+and+try+again.</location>
	</error-page>

	<error-page>
		<error-code>401</error-code> <!-- Unauthorized -->
		<location>/pages/error/error_page.jsp?error=401&amp;msg=Unauthorized.+Please+login+to+access+this+resource.</location>
	</error-page>

	<error-page>
		<error-code>403</error-code> <!-- Forbidden -->
		<location>/pages/error/error_page.jsp?error=403&amp;msg=Access+Forbidden.+You+do+not+have+permission+to+access+this+resource.</location>
	</error-page>

	<error-page>
		<error-code>404</error-code> <!-- Not Found -->
		<location>/pages/error/error_page.jsp?error=404&amp;msg=Page+Not+Found.+The+requested+page+does+not+exist.</location>
	</error-page>

	<error-page>
		<error-code>500</error-code> <!-- Internal Server Error -->
		<location>/pages/error/error_page.jsp?error=500&amp;msg=Internal+Server+Error.+Please+try+again+later.</location>
	</error-page>

	<error-page>
		<error-code>503</error-code> <!-- Service Unavailable -->
		<location>/pages/error/error_page.jsp?error=503&amp;msg=Service+Unavailable.+Please+try+again+later.</location>
	</error-page>


</web-app>
